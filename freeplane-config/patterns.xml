<!-- Default patterns supplied with FreeMind -->
<patterns>
   <pattern name="Default">
      <node>
         <font/>
      </node>
      <edge/>
   </pattern>
   <pattern name="Normal">
      <node color="#000000"/>
   </pattern>
   <!-- task -->
   <pattern name="Author">
      <node color="#338800"/> <!-- green -->
   </pattern>
   <pattern name="Title">
      <node color="#090f6b"/> <!-- dark blue -->
   </pattern>
   <pattern name="Excerpt">
      <node color="#166799"/>
   </pattern>
   <pattern name="Comment">
      <node color="#8b12d6"/>
   </pattern>
   <!-- document -->
   <pattern name="Summary">
      <node color="#999999"/>
   </pattern>
   <pattern name="Citation">
      <node color="#ff33b8"/>
   </pattern>
   <pattern name="Topic">
      <node color="#669900"/>
   </pattern>
   <pattern name="Larger Topic">
      <node color="#008844"/>
   </pattern>
   <pattern name="Waiting Topic">
      <node color="#B3B95C"/>
   </pattern>
   <pattern name="Object / Keyword">
      <node color="#996600"/>
   </pattern>
   <pattern name="Object of Code">
      <!-- that is function name, database table name, class name etc.-->
      <node color="#CC6600"/>
   </pattern>
   <pattern name="Question">
      <node color="#999900"/>
   </pattern>
   <pattern name="Open Question">
      <node color="#CC3300"/>
   </pattern>
   <pattern name="Resolved">
      <node color="#990099"/>
   </pattern>
   <!-- These two examples are not really meant for serious use -->
   <!-- example of edges -->
   <pattern name="Blue with edges">
      <node color="#000099"/>
      <edge color="#009999" width="4"/>
   </pattern>
   <!-- example with text
        Because this we have no undo, this example is dangerous.
        We'd better comment it out.
   <pattern name="Program">
      <node color="#996600" italic="true"  text="Program">
         <font size="16" italic="true"/>
      </node>
   </pattern>
   -->
</patterns>
